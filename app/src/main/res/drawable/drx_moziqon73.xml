<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape android:shape="rectangle">
            <corners android:radius="10dp" />
            <gradient
                android:angle="315"
                android:endColor="@color/drx_moziqonwhite"
                android:startColor="@color/drx_moziqonwhite"
                android:type="linear" />
        </shape>
    </item>

    <item
        android:bottom="0dp"
        android:left="0dp"
        android:right="0dp"
        android:top="0dp">
        <shape android:shape="rectangle">
            <corners android:radius="9dp" />
            <gradient
                android:angle="90"
                android:endColor="#FFD9C2"
                android:startColor="#FFFFFF"
                android:type="linear"
                android:useLevel="false" />
        </shape>
    </item>

</layer-list>
